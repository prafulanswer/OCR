    <script>
        (function($) {
            "use strict";

            jQuery('#vmap').vectorMap({
                map: 'world_en',
                backgroundColor: null,
                color: '#ffffff',
                hoverOpacity: 0.7,
                selectedColor: '#1de9b6',
                enableZoom: true,
                showTooltip: true,
                values: sample_data,
                scaleColors: ['#1de9b6', '#03a9f5'],
                normalizeFunction: 'polynomial'
            });
        })(jQuery);
    </script>
    <script>
        ( function ( $ ) {
          "use strict";
              //Sales chart
      var ctx = document.getElementById( "sales-chart" );
      ctx.height = 150;
      var myChart = new Chart( ctx, {
          type: 'line',
          data: {
              labels: [ "06-04-19", "06-04-19", "06-04-19", "06-04-19", "06-04-19", "06-04-19", "06-04-19" ],
              type: 'line',
              defaultFontFamily: 'Montserrat',
              datasets: [ {
                  label: "Actual",
                  data: [ 20, 21, 22, 23, 24, 25, 26 ],
                  backgroundColor: 'transparent',
                  borderColor: 'rgba(220,53,69,0.75)',
                  borderWidth: 3,
                  pointStyle: 'circle',
                  pointRadius: 5,
                  pointBorderColor: 'transparent',
                  pointBackgroundColor: 'rgba(220,53,69,0.75)',
                      }, {
                  label: "Your",
                  data: [ 28, 29, 30, 29, 28, 29, 30 ],
                  backgroundColor: 'transparent',
                  borderColor: 'rgba(40,167,69,0.75)',
                  borderWidth: 3,
                  pointStyle: 'circle',
                  pointRadius: 5,
                  pointBorderColor: 'transparent',
                  pointBackgroundColor: 'rgba(40,167,69,0.75)',
                      } ]
          },
          options: {
              responsive: true,
  
              tooltips: {
                  mode: 'index',
                  titleFontSize: 12,
                  titleFontColor: '#000',
                  bodyFontColor: '#000',
                  backgroundColor: '#fff',
                  titleFontFamily: 'Montserrat',
                  bodyFontFamily: 'Montserrat',
                  cornerRadius: 3,
                  intersect: false,
              },
              legend: {
                  display: false,
                  labels: {
                      usePointStyle: true,
                      fontFamily: 'Montserrat',
                  },
              },
              scales: {
                  xAxes: [ {
                      display: true,
                      gridLines: {
                          display: false,
                          drawBorder: false
                      },
                      scaleLabel: {
                          display: true,
                          labelString: 'date'
                      }
                          } ],
                  yAxes: [ {
                      display: true,
                      gridLines: {
                          display: false,
                          drawBorder: false
                      },
                      scaleLabel: {
                          display: true,
                          labelString: 'BMI'
                      }
                          } ]
              },
              title: {
                  display: false,
                  text: 'Normal Legend'
              }
          }
      } );

       //radar chart
    var interest = '<%- userProfile[0].interest%>';
    var interestString = "";
    var interestList = [];

    for (var i= 0; i < interest.length;i++){
        if (interest[i] == ",")
        {
            interestList.push(interestString);
            interestString = "";
        }else {
        interestString = interestString.concat(interest[i]);
        }
    }
    interestList.push(interestString);
    
    var ctx = document.getElementById( "radarChart" );
    ctx.height = 160;
    var myChart = new Chart( ctx, {
        type: 'radar',
        data: {
            labels: interestList,
            datasets: [
                {
                    label: "Even day",
                    data: [ 65, 59, 66, 45, 56, 55, 40 ],
                    borderColor: "rgba(0, 123, 255, 0.6)",
                    borderWidth: "1",
                    backgroundColor: "rgba(0, 123, 255, 0.4)"
                            },
                {
                    label: "Odd Day",
                    data: [ 28, 12, 40, 19, 63, 27, 87 ],
                    borderColor: "rgba(0, 123, 255, 0.7",
                    borderWidth: "1",
                    backgroundColor: "rgba(0, 123, 255, 0.5)"
                            }
                        ]
        },
        options: {
            legend: {
                position: 'top'
            },
            scale: {
                ticks: {
                    beginAtZero: true
                }
            }
        }
    } );

        //bar chart
        var ctx = document.getElementById( "barChart" );
    //    ctx.height = 200;
    var myChart = new Chart( ctx, {
        type: 'bar',
        data: {
            labels: [ "January", "February", "March", "April", "May", "June", "July" ],
            datasets: [
                {
                    label: "BMI Actual",
                    data: [ 65, 59, 80, 81, 56, 55, 40 ],
                    borderColor: "rgba(0, 123, 255, 0.9)",
                    borderWidth: "0",
                    backgroundColor: "rgba(0, 123, 255, 0.5)"
                            },
                {
                    label: "BMI Expected",
